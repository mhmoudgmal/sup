{
    "localstack_config": {
        "version": "0.10.8",
        "services": ["lambda"],
        "lambda_executer": "docker-reuse",
        "docker_host": "unix:///var/run/docker.sock"
    },
    "services": {
        "nodejs": {
            "runtime": "nodejs10.x",
            "handler": "index.handler",
            "env_file": ".env.test",
            "function_name": "test_node_function",
            "env_vars": {
                "BAR": "bar",
                "BAZ": "baz"
            },
            "files": ["index.js", "src"],
            "function_path": "nodejs"
        },
        "ruby": {
            "runtime": "ruby2.5",
            "handler": "handler.handler",
            "env_file": ".env.test",
            "function_name": "test_ruby_function",
            "env_vars": {
                "BAR": "bar",
                "BAZ": "baz"
            },
            "files": ["handler.rb"],
            "function_path": "ruby"
        }
    }
}
